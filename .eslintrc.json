{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue", "jest"],
  "rules": {
    "no-unused-vars": ["warn", {"vars": "all", "args": "after-used"}]
  },
  "globals": {
    "IS_DEV": true
  }
}
