{
  "name": "homepage",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev-old:client": "webpack --config webpack.dev.js --watch",
    "dev-old:ssr": "webpack --config webpack.server.js --watch --mode=development",
    "dev-old:server": "nodemon ./dist/server/run-server.js",
    "dev:client": "vite build --mode=development --watch --outDir dist/client",
    "dev:server": "PHASE=dev nodemon --exec tsx server.ts",
    "dev": "npm-run-all -p 'dev:*'",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build-old:client": "NODE_OPTIONS=\"--openssl-legacy-provider\" yarn webpack --config webpack.prod.js",
    "build:ssr": "webpack --config webpack.server.js --mode=production",
    "serve-old": "tsx ./dist/server/run-server.js",
    "serve": "PHASE=production tsx server.ts",
    "build": "tsx build.js",
    "test": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.16.7",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^6.5.1",
    "eslint": "^8",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-vue": "^7.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.1",
    "husky": "^4.3.5",
    "jest": "^27.4.7",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^2.5.2",
    "mock-fs": "^4.10.4",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "port-pid": "0.0.7",
    "postcss": "^8.4.47",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.2.0",
    "pug-loader": "^2.4.0",
