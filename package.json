    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-plugin-vue": "^7.1.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "mock-fs": "^4.10.4",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "port-pid": "0.0.7",
    "postcss": "^8.4.47",
    "prettier": "^2.2.0",
    "tsx": "^4.19.2",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "axios": "^1.6.0",
    "date-fns": "^2.9.0",
    "express": "^4.17.1",
    "feather-icons": "^4.28.0",
    "front-matter": "^3.1.0",
    "highlight.js": "^10.4.1",
    "katex": "^0.16.10",
    "marked": "^4.0.10",
    "sass": "^1.25.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^4.5.5",
    "vue": "^3.5.12",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "xml-js": "^1.6.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": "eslint --fix",
    "**/*.json": "prettier --write",
    ".babelrc": "prettier --write"
