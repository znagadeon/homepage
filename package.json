    "speed-measure-webpack-plugin": "^1.5.0",
    "tsx": "^4.19.2",
    "vite": "^5.4.10",
    "vue-loader": "^17.0.0",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^2.5.2"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "axios": "^1.6.0",
    "date-fns": "^2.9.0",
    "express": "^4.17.1",
    "feather-icons": "^4.28.0",
    "front-matter": "^3.1.0",
    "highlight.js": "^10.4.1",
    "katex": "^0.16.10",
    "marked": "^4.0.10",
